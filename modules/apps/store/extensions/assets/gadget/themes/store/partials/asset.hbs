<script>
    var isSocial = ('{{isSocial}}' == 'true');
</script>
<div class="asset-description">
    {{#with asset}}
        {{#attributes}}
            <div class="asset-description-header">
            <div class="row">{{dump .}} </div>
                <div class="row">
                    <div class="span9">
                    <h3>{{overview_name}}</h3>
                    <table class="table table-striped table-hover">
                    <tr>
                    <td>{{t "Description"}} </td>
                    <td>{{overview_description}} </td>
                    </tr>
                    <tr>
                    <td>{{t "Name"}} </td>
                    <td>{{overview_name}} </td>
                    </tr>
                    <tr>
                    <td>{{t "Version"}} </td>
                    <td>{{overview_version}} </td>
                    </tr>
                    <tr>
                    <td>{{t "URL"}} </td>
                    <td>{{overview_url}} </td>
                    </tr>
                    <tr>
                    <td>{{t "Category"}} </td>
                    <td>{{overview_category}} </td>
                    </tr>

                    </table>
                    </div>

                </div>



            </div>

            <div class="assetp-properties">

                <ul class="nav nav-tabs" id="assetp-tabs" data-aid="{{../path}}">
                    <li class="active">
                        <a href="#tab-github" data-toggle="tab" data-type="basic">{{t "GitHub Summary "}}</a>
                    </li>
                    <li>
                        <a href="#tab-committer" data-toggle="tab" data-type="comments">{{t "GitHub Committer"}}</a>
                    </li>
                      <li>
                        <a href="#tab-qa" data-toggle="tab" data-type="comments">{{t "QA Summary"}}</a>
                    </li>
                    <li>
                        <a href="#tab-bamboo" data-toggle="tab" data-type="comments">{{t "Bamboo Summary"}}</a>
                    </li>
                </ul>

                <div class="tab-content">
                    <div class="tab-pane active" id="tab-github">
                        <div class="content">
                        <iframe  height=500px width=800px src="http://10.100.5.41:9764/gadgets/ifr?url=http://10.100.5.41:9764/portal/gadgets/git-commit-summary/git-commit-summary.xml&onRemote=true"></iframe>
                       </div>
                    </div>
                    <div class="tab-pane" id="tab-committer">
                        <iframe height=500px width=800px src="http://10.100.5.41:9764/gadgets/ifr?url=http://10.100.5.41:9764/portal/gadgets/git-project-summary/git-project-summary.xml&onRemote=true"></iframe>
                    </div>
                    <div class="tab-pane" id="tab-qa">
                        <iframe style="height:500px;width:500px;" src="http://10.100.5.44:9763/gadgets/ifr?url=http://10.100.5.44:9763/portal/gadgets/test-status/test-status.xml&onRemote=true&productId=3&productVersionId=33"></iframe>
                    </div>
                    <div class="tab-pane" id="tab-bamboo">
                        <iframe height=500px width=800px src="http://10.100.5.41:9764/gadgets/ifr?url=http://10.100.5.41:9764/portal/gadgets/bamboo-project-summary-new/bamboo-project-summary-new.xml&onRemote=true"></iframe>
                    </div>
                </div>

            </div>
        {{/attributes}}
    {{/with}}
</div>

