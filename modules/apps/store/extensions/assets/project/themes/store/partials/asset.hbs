<script>
    var isSocial = ('{{isSocial}}' == 'true');
</script>
<div>
    {{#with asset}} {{#attributes}}

        <div class="row">
            <div class="span12">
                <h3>{{overview_name}}</h3>
                <table width="100%" class="table table-striped table-hover">

                    <tr>
                        <td>{{t "Name"}}</td>
                        <td>{{overview_name}}</td>
                    </tr>
                    <tr>
                        <td>{{t "Description"}}</td>
                        <td>{{overview_description}}</td>
                    </tr>
                    <tr>
                        <td>{{t "Version"}}</td>
                        <td>{{overview_version}}</td>
                    </tr>
                    <tr>
                        <td>{{t "Version Control"}}</td>
                        <td>{{other_versioncontrol}}</td>
                    </tr>
                    <tr>
                        <td>{{t "Issue Tracker"}}</td>
                        <td>{{other_issuetracker}}</td>
                    </tr>
                    <tr>
                        <td>{{t "Project Management Tool"}}</td>
                        <td>{{other_projectmanagementtool}}</td>
                    </tr>
                    <tr>
                        <td>{{t "Continuous Integration"}}</td>
                        <td>{{other_continuousintegration}}</td>
                    </tr>

                </table>
            </div>

        </div>


     </br></br>
    <div class="assetp-properties">

        <ul class="nav nav-tabs" id="assetp-tabs" >
            <li class="active">
                <a href="#tab-github" data-toggle="tab" data-type="basic">{{t "GitHub"}}</a>
            </li>
            <li>
                <a href="#tab-committer" data-toggle="tab" data-type="comments">{{t "GitHub Committer"}}</a>
            </li>
            <li>
                <a href="#tab-qa" data-toggle="tab" data-type="comments">{{t "QA "}}</a>
            </li>
            <li>
                <a href="#tab-bamboo" data-toggle="tab" data-type="comments">{{t "Bamboo "}}</a>
            </li>
        </ul>

        <div class="tab-content">
            <div class="tab-pane active" id="tab-github">
                   <iframe height=500px width="100%" src="http://10.100.5.41:9764/gadgets/ifr?url=http://10.100.5.41:9764/portal/gadgets/git-commit-summary/git-commit-summary.xml&onRemote=true&gitID=jaggery"></iframe>

            </div>
            <div class="tab-pane" id="tab-committer">
                <iframe height=500px width="100%"  src="http://10.100.5.41:9764/gadgets/ifr?url=http://10.100.5.41:9764/portal/gadgets/git-project-summary/git-project-summary.xml&onRemote=true&gitID=jaggery"></iframe>
            </div>
            <div class="tab-pane" id="tab-qa">
                <iframe height=800px width="100%" src="http://10.100.5.44:9763/gadgets/ifr?url=http://10.100.5.44:9763/portal/gadgets/test-status/test-status.xml&onRemote=true&productId=3&productVersionId=33"></iframe>
            </div>
            <div class="tab-pane" id="tab-bamboo">
                <iframe height=500px width="100%" src="http://10.100.5.41:9764/gadgets/ifr?url=http://10.100.5.41:9764/portal/gadgets/bamboo-project-summary-new/bamboo-project-summary-new.xml&onRemote=true&bambooID=WSO2CARBON"></iframe>
            </div>
        </div>

    </div>
    {{/attributes}} {{/with}}
</div>
